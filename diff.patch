+    
+    private void setupMenuBar() 
+    {
+        JMenuBar menuBar = new JMenuBar();
+        JMenu menu = new JMenu("Controles");
+        
+        JMenuItem reconnect = new JMenuItem("Reconectar");
+        
+        JMenuItem exit = new JMenuItem("Sair");
+        exit.addActionListener(e -> System.exit(0));
+        
+        menu.add(reconnect);
