+        
+        panel = new DisplayPanel();
+        add(panel);
+        
+        // Adiciona uma barra de menu
+        setupMenuBar();
+
+        // Instancia os atributos para valores padr√£o
+        fluxo = new FluxoAgua();
+        diametroEntrada = 0.0;
+        velmediaFluxoAgua = 0.0;
