+
+    private void loadImages() 
+    {
+        try 
+        {
+            // Tenta carregar do sistema de arquivos (pasta images na raiz do projeto)
+
+            File fundoFile = new File("images/Hidrometro.png");
+            File ponteiroFile = new File("images/Ponteiro.png");
