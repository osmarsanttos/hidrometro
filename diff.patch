+package src;
+
+// Defini√ß√£o da classe que realiza as opera√ß√µes para o volume acumulado de √°gua
+class FluxoAgua 
+{
+    private double volume;  
+
+    // Construtor padr√£o
+    public FluxoAgua() 
+    {
+        this.volume = 0.0;
+    }
+
+    // get e set
+    public double getVolume() 
+    {
+        return volume;
+    }
+
+    private void setVolume(double vol)
+    {
+        volume += vol;
+    }
+    
+    // Opera√ß√£o de medi√ß√£o de volume a partir do di√¢metro de entrada e velocidade m√©dia de fluxo de √°gua
+    private double medicao(double dE, double vmFA)
+    {
+        double area = Math.PI * (dE * dE / 4);
+
+        return (area * vmFA * 3600);
+    }
+ 
+    // Seta o volume ap√≥s aplicado fator oscilat√≥rio
+    public void atualizar(int tempo, double dE, double vmFA)
+    {
+        int horaDoDia = tempo % 24; // Atualiza o tempo para horas do dia em um padr√£o de 00h a 23h
+    
+        double oscillacao = (Math.sin((2* Math.PI * horaDoDia / 24.0) - (Math.PI / 2)) + 1.0) / 2.0; 
+        double fatorOscilatorio = 0.1 + (oscillacao * 0.9); // Calcula uma oscila√ß√£o para simular a varia√ß√£o do consumo de √°gua ao longo do dia
+        
+        double velocidadeOscilatoria = vmFA * fatorOscilatorio; // Aplica a oscila√ß√£o na velocidade m√©dia de fluxo de √°gua 
+        double vol = medicao(dE, velocidadeOscilatoria);
+        
+        setVolume(vol);
+    }
+}
